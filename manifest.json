{
  "manifest_version": 2,

	"name": "Youtube Thumbnail Preview",
	"description": "View youtube thumbnails with the click of a button.",
	"version": "1.2",
	"permissions":
	[
    "activeTab",
    "tabs"
	],
	"browser_action": {
   "default_popup": "popup.html",
   "default_icon": "assets/icon.png",
   "default_title": "Show thumbnail"
   }
}
